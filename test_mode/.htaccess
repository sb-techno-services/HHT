Options +FollowSymLinks

RewriteEngine on
RewriteCond %{HTTP_HOST} ^hanumanhindutemple.org [NC]
RewriteRule ^(.*)$ https://www.hanumanhindutemple.org/$1 [L,R=301,NC] 


RewriteEngine On
RewriteRule ^event_([0-9]+).html /test_mode/events_inner.php?j_id=$1 [QSA,L]
RewriteRule ^news_([0-9]+).html /test_mode/newsinner.php?j_id=$1 [QSA,L]
RewriteRule ^pooja_details_([0-9]+).html /test_mode/pooja_details.php?j_id=$1 [QSA,L]
RewriteRule ^donation_details_([0-9]+).html /test_mode/donate_details.php?j_id=$1 [QSA,L]




